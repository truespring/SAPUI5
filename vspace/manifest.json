{
    "sap.app": {
		"id": "openUI5",
        "applicationVersion": {
        	"version": "1.0.0"
        },
        "i18n": "i18n/i18n.properties"
    },
    "sap.ui5": {
		"rootView": {
			"viewName": "OpenUI5.view.frame.Layout",
			"type": "XML",
			"async": true
		},
        "dependencies": {
        	"libs": {
        		"sap.ui.layout": {},
				"sap.m": {},
				"sap.ui.table": {}
            }
        }, 
        "resources": {
        	"css" : [{ "uri": "css/style.css", "id": "css" }]
        },
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"feed.json",
					"manifest.json"
				]
			}
		},
        "models": {
        	"i18n": {
        		"type": "sap.ui.model.resource.ResourceModel",
                "settings": {
				"bundleName": "OpenUI5.i18n.i18n"
				}
            }
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"controlId": "centerView",
				"controlAggregation": "pages",
				"transition": "show",
				"async": true
			},
			"routes": [{ "name": "contents1",       "pattern": ""         ,         "target": "contents1"},
			           { "name": "contents2",       "pattern": "contents2",         "target": "contents2"},
			           { "name": "contents3",       "pattern": "contents3",         "target": "contents3"},
			           { "name": "contents4",       "pattern": "contents4",         "target": "contents4"},
			           { "name": "contents6",       "pattern": "contents6",         "target": "contents6"},
			           { "name": "contents5",       "pattern": "contents5",         "target": "contents5"},
			           { "name": "contents7",       "pattern": "contents7",         "target": "contents7"},
			           { "name": "contents8",       "pattern": "contents8",         "target": "contents8"}
			          ],
			"targets": {
				"contents1": {
					"viewPath": "OpenUI5.view.contents",
					"viewName": "Contents1",
					"viewLevel": 1,
					"viewId": "contents1"
				},
				"contents2": {
					"viewPath": "OpenUI5.view.contents",
					"viewName": "Contents2",
					"viewLevel": 1,
					"viewId": "contents2"
				},
                "contents3": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents3",
                    "viewLevel": 1,
                    "viewId": "contents3"
                },
                "contents4": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents4",
                    "viewLevel": 1,
                    "viewId": "contents4"
                },
                "contents5": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents5",
                    "viewLevel": 1,
                    "viewId": "contents5"
                },
                "contents6": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents6",
                    "viewLevel": 1,
                    "viewId": "contents6"
                },
                "contents7": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents7",
                    "viewLevel": 1,
                    "viewId": "contents7"
                },
                "contents8": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents8",
                    "viewLevel": 1,
                    "viewId": "contents8"
                }
			}
		}
    }
}