<mvc:View
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    controllerName="OpenUI5.controller.frame.Left">

    <l:HorizontalLayout>
        <l:content>
            <VBox>
                <!-- <Image              class="mvpLogoType"     src="./images/Logo.png"/> -->
                <l:VerticalLayout   class="menuListBox">
                <!-- Quick Start -->
                    <VBox class="responsiveMenu">
                        <l:HorizontalLayout>
                            <!-- <Image  src="./images/Ico_List.png"/> -->
                            <Text   text="Quick Start"    class="subTitle"/>
                        </l:HorizontalLayout>
                        <Button     id="contents1"           text="Air Pollution"           press=".onNavToContent1"          class="btnLeftList"/>
                   <!-- <Button     id="contents2"           text="Case Studies 2"          press=".onNavToContent2"          class="btnLeftList"/> -->
                   <!-- <Button     id="contents3"           text="Bar SampleChart"         press=".onNavToContent3"          class="btnLeftList"/> -->
                   <!-- <Button     id="contents4"           text="Bullet SampleChart"      press=".onNavToContent4"          class="btnLeftList"/> -->
                        <Button     id="sensorSparkRT"       text="Sensor Spark Real-time"  press=".onNavToSensorSparkRT"     class="btnLeftList"/>
                        <Button     id="sparkController"     text="Spark Controller"        press=".onNavToSparkController"   class="btnLeftList"/>
                        <Button     id="cpuInfo"             text="Cpu Info"                press=".onNavToCpuInfo"           class="btnLeftList"/>
                        <Button     id="sapui5"              text="SAPUI5"                  press=".onNavToSapUI5"            class="btnLeftList"/>
                        <Button     id="python_ml"           text="Python Machine Learning" press=".onNavToPythonML"          class="btnLeftList"/>
                        <Button     id="awx"                 text="AWX(admin/password)"     press=".onNavToAWX"               class="btnLeftList"/>
                        <Button     id="ambari"              text="Ambari(admin/admin123)"  press=".onNavToAmbari"            class="btnLeftList"/>
                        <Button     id="grafana"             text="Grafana(admin/admin)"    press=".onNavToGrafana"           class="btnLeftList"/>
                        <Button     id="prometheus"          text="Prometheus Server"       press=".onNavToPrometheus"        class="btnLeftList"/>
                        <Button     id="prometheus_fsimage"  text="Prometheus FSImage"      press=".onNavToPrometheusFSImage" class="btnLeftList"/>
                        <Button     id="prometheus_node"     text="Prometheus Node"         press=".onNavToPrometheusNode"    class="btnLeftList"/>
                    </VBox>
                </l:VerticalLayout>
            </VBox>
        </l:content>
    </l:HorizontalLayout>
    
</mvc:View>