{
	"_version": "1.21.0",
    "sap.app": {
		"id": "openUI5",
        "applicationVersion": {
        	"version": "1.0.0"
        }
    },
    "sap.ui5": {
		"rootView": {
			"viewName": "OpenUI5.ChartContainer",
			"type": "XML",
			"async": true
		},
        "dependencies": {
        	"libs": {
        		"sap.ui.layout": {},
				"sap.m": {},
				"sap.ui.table": {}
            }
        }, 
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"manifest.json"
				]
			}
		},
        "models": {
			"chartList": {
			  "type": "sap.ui.model.json.JSONModel",
			  "uri": "ChartContainerData.json"
			}
        }
    }
}